<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">

    <title>FoodExp</title>

    <!-- Base Css Files -->
    <link href="/stylesheets/bootstrap.css"
          rel="stylesheet"/>

    <!-- Font Icons -->
    <link href="/javascripts/font-awesome/css/font-awesome.min.css"
          rel="stylesheet"/>
    <link href="/stylesheets/material-design-iconic-font.min.css"
          rel="stylesheet">

    <!-- animate css -->
    <link href="/stylesheets/animate.css"
          rel="stylesheet"/>

    <!-- Waves-effect -->
    <link href="/stylesheets/waves-effect.css"
          rel="stylesheet">

    <!-- Custom Files -->
    <link href="/stylesheets/helper.css"
          rel="stylesheet"
          type="text/css"/>
    <link href="/stylesheets/style.css"
          rel="stylesheet"
          type="text/css"/>

    <link href="javascripts/datatables/jquery.dataTables.min.css"
          rel="stylesheet"
          type="text/css"/>

    <style type="text/css">
        tfoot {
            display: table-header-group;
        }

        tfoot input {
            width: 100%;
        }
    </style>

</head>
<body class="fixed-left">


<div id="wrapper">


    <div class="topbar">
        <!-- LOGO -->
        <div class="topbar-left">
            <div class="text-center">
                <a href="#"
                   class="logo">
                    <img src="/images/icon.png"
                         alt=""
                         class="thumb-sm">
                    <span> FoodExp </span>
                </a>
            </div>
        </div>

        <div class="navbar navbar-default"
             role="navigation">
            <div class="container">
                <div class="">
                    <div class="pull-left">
                        <button class="button-menu-mobile open-left">
                            <i class="fa fa-bars"></i>
                        </button>
                        <span class="clearfix"></span>
                    </div>
                </div>

            </div>
        </div>
    </div>

    <div class="left side-menu">
        <div class="sidebar-inner slimscrollleft">
            <div class="user-details">
                <div class="pull-left">
                    <img src="/images/chef.png" alt="" class="thumb-md img-circle">
                </div>
                <div class="user-info">
                    <div class="dropdown m-t-20">
                        <p><strong>Chef</strong>
                        </p>
                    </div>

                </div>
            </div>

            <div id="sidebar-menu">
                <ul>
                    <li>
                        <a href="/" class="waves-effect active">
                            <i class="fa fa-home"></i>
                            <span> Home </span>
                        </a>
                    </li>
                    <li>
                        <a href="/notifikasis/"
                           class="waves-effect">
                            <i class="fa fa-bell"></i>
                            <span> Notifikasi </span>
                        </a>
                    </li>
                    <li>
                        <a href="/pembelians/" class="waves-effect">
                            <i class="fa fa-shopping-cart"></i>
                            <span> Pembelian </span>
                        </a>
                    </li>
                    <li>
                        <a href="/penyimpanans/" class="waves-effect">
                            <i class="fa fa-archive"></i>
                            <span> Penyimpanan </span>
                        </a>
                    </li>

                </ul>
            </div>
        </div>
    </div>

    <div class="content-page">

        <div class="content">
            <div class="container">

                <!-- Page-Title -->
                <div class="row">
                    <div class="col-sm-12">
                        <ol class="breadcrumb pull-left">
                            <li><a href="/">Home</a>
                            </li>
                        </ol>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">Home</h3>
                            </div>

                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-md-12 col-sm-12 col-xs-12">

                                        <div class="alert alert-warning" role="alert">
                                            <i class="fa fa-warning"></i> Tep Terigu pembelian 10 April 2017 akan
                                            kadaluarsa 7 hari lagi!
                                        </div>
                                        <div class="alert alert-danger" role="alert">
                                            <i class="fa fa-times-circle"></i> Tepung Terigu pembelian 1 April 2017 hari
                                            ini kadaluarsa!
                                        </div>

                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="m-b-30">
                                            <a href="#" class="btn btn-primary waves-effect waves-light">
                                                <i class="fa fa-bell-o"></i><span> Daftar Notifikasi </span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>

            </div>

        </div>


    </div>

    <footer class="footer text-right">
        <%= new Date().getFullYear() %>
        Â© Home Industry
    </footer>

</div>


<script>
    var resizefunc = [];
</script>


<script src="/javascripts/jquery.min.js" type="text/javascript"></script>
<script src="/javascripts/bootstrap.js" type="text/javascript"></script>
<script src="/javascripts/waves.js" type="text/javascript"></script>
<script src="/javascripts/jquery-detectmobile/detect.js" type="text/javascript"></script>
<script src="/javascripts/fastclick/fastclick.js" type="text/javascript"></script>
<script src="/javascripts/jquery-slimscroll/jquery.slimscroll.js" type="text/javascript"></script>


<script src="/javascripts/jquery.app.js"></script>

</body>
</html>
